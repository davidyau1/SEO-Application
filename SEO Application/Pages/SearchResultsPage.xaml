<Page x:Class="SEO_Application.Pages.SearchResultsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SEO_Application.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="SearchResultsPage">

    <Grid>
        <Label Content="Results" HorizontalAlignment="Center" VerticalAlignment="Top"/>

        <Label Content="SearchTerm" HorizontalAlignment="Left" Margin="50,30,0,0" VerticalAlignment="Top"/>
        
        <Label HorizontalAlignment="Left" Margin="159,33,0,0" Content="{Binding KeyWord}" VerticalAlignment="Top" Width="311"/>

        <Label Content="Url" HorizontalAlignment="Left" Margin="50,60,0,0" VerticalAlignment="Top"/>
        <Label HorizontalAlignment="Left" Margin="159,63,0,0" Content="{Binding Url}" VerticalAlignment="Top" Width="311"/>


        <Label Content="Results" HorizontalAlignment="Left" Margin="50,90,0,0" VerticalAlignment="Top"/>
        <Label HorizontalAlignment="Left" Margin="159,93,0,0" Content="{Binding Results}" VerticalAlignment="Top" Width="311"/>


    </Grid>
</Page>
